<% layout('index') %>
<!-- Form horizontal -->
<div class="">
    <div class="panel-body bg-info">
        <div class="text-left col-lg-6">
            <div class="col-lg-3">
                <h4>Order List</h4>
            </div>
            <div class="col-lg-6">
                <input type="" id="name" class="form-control" placeholder="Search order id"/>
            </div>
        </div>
        <div class="text-right col-lg-6">
            <a href="/order/add" class="btn btn-primary">Add new order</a>
        </div>
    </div>
    <div class="bg-primary search-panel">

        <div class="col-lg-3">
            <label>Oder date(from):</label>
            <input type="number" id="priceFrom" class="form-control" placeholder="Oder date"/>
        </div>
        <div class="col-lg-3">
            <label>Oder to:(to)</label>
            <input type="number" id="priceTo" class="form-control" placeholder="Oder to"/>
        </div>
        <div class="col-lg-3">
            <label>Price from</label>
            <input type="number" id="skufrm" class="form-control" placeholder="Min price"/>
        </div>
        <div class="col-lg-3">
            <label>Price upto:</label>
            <input type="number" id="skuto" class="form-control" placeholder="MAX price"/>
        </div>
    </div>
    <div class="clearfix"></div>
    <table class=" table table-striped table-hover">
        <thead>
        <tr>
            <th>#</th>
            <th>Item Name</th>
            <th>Item SKU</th>
            <th>Regular Price</th>
            <th>Image</th>
            <th>Action</th>
        </tr>
        </thead>
    </table>
</div>